{
    "kind": "BuildConfig",
    "apiVersion": "v1",
    "metadata": {
        "name": "architect",
        "creationTimestamp": null,
        "labels": {
            "name": "architect"
        },
        "annotations": {
        }
    },
    "spec": {
        "triggers": [],
        "runPolicy": "Serial",
        "source": {
            "type": "Git",
            "git": {
                "uri": "https://github.com/Skatteetaten/architect.git"
            }
        },
        "strategy": {
            "type": "Custom",
            "customStrategy": {
                "from": {
                    "kind": "DockerImage",
                    "name": "docker-registry.aurora.sits.no:5000/aurora/cotterpin:0.0.7"
                },
                "env": [
                    {
                        "name": "GO_PACKAGE",
                        "value": "github.com/skatteetaten/architect"
                    },
                    {
                        "name": "PUSH_EXTRA_TAGS",
                        "value": "latest major minor patch"
                    }
                ],
                "exposeDockerSocket": true
            }
        },
        "output": {
            "to": {
                "kind": "DockerImage",
                "name": "docker-registry.aurora.sits.no:5000/aurora/architect"
            }
        },
        "resources": {},
        "postCommit": {},
        "nodeSelector": null
    },
    "status": {
        "lastVersion": 0
    }
}
